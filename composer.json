{
    "name": "szabacsik/identity",
    "description": "Identity Immutable Value Object",
    "type": "library",
    "require": {
        "php": "^7.2 || ^8.0",
        "ramsey/uuid": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "András Szabácsik",
            "homepage": "http://github.com/szabacsik/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Szabacsik\\Identity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Szabacsik\\Identity\\Test\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "phpunit --verbose --colors=always",
        "phpunit-coverage": "phpunit --verbose --colors=always --coverage-html build/coverage",
        "test": [
            "@phpunit"
        ]
    }
}
